  "scripts": {
    "dev": "cross-env MODE_ENV=dev node --max-old-space-size=4096 .electron-vue/dev-runner.js",
    "dev:test": "cross-env MODE_ENV=test node --max-old-space-size=4096 .electron-vue/dev-runner.js",
    "dev:pre": "cross-env MODE_ENV=pre node --max-old-space-size=4096 .electron-vue/dev-runner.js",
    "dev:gray": "cross-env MODE_ENV=gray node --max-old-space-size=4096 .electron-vue/dev-runner.js",
    "dev:prod": "cross-env MODE_ENV=prod node --max-old-space-size=4096 .electron-vue/dev-runner.js",
    "build": "cross-env MODE_ENV=dev node --max-old-space-size=4096 .electron-vue/build.js && electron-builder -c ./electron-builder.yml",
    "build:test": "cross-env MODE_ENV=test node --max-old-space-size=4096 .electron-vue/build.js && electron-builder -c ./electron-builder.yml",
    "build:pre": "cross-env MODE_ENV=pre node --max-old-space-size=4096 .electron-vue/build.js && electron-builder -c ./electron-builder.yml",
    "build:gray": "cross-env MODE_ENV=gray node --max-old-space-size=4096 .electron-vue/build.js && electron-builder -c ./electron-builder.yml",
    "build:prod": "cross-env MODE_ENV=prod node --max-old-space-size=4096 .electron-vue/build.js && electron-builder -c ./electron-builder.yml",
    "build:dir": "node .electron-vue/build.js && electron-builder -c ./electron-builder.yml --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack:win-dev": "npm run build -- -w",
    "pack:mac-dev": "npm run build -- -m",
    "pack:win-test": "npm run build:test -- -w",
    "pack:mac-test": "npm run build:test -- -m",
    "pack:win-pre": "npm run build:pre -- -w",
    "pack:mac-pre": "npm run build:pre -- -m",
    "pack:win-prod": "npm run build:prod -- -w",
    "pack:mac-prod": "npm run build:prod -- -m",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix && electron-builder install-app-deps",
    "devtools": "./node_modules/.bin/vue-devtools"
  },
